#Summoning/Tagging
tag @e[type=item,nbt={OnGround:1b,Item:{id:"minecraft:stone"}}] add stone
execute as @e[type=item,tag=stone] at @s if block ~ ~-1 ~ minecraft:furnace[lit=false] run tag @s add approve1
execute as @e[type=item,tag=stone,tag=approve1] at @s run summon item_frame ~ ~-2 ~ {Tags:["grindersummon"]}
execute as @e[type=item,tag=stone,tag=approve1] at @s run kill @e[type=item,tag=stone,tag=approve1,sort=nearest,limit=1]
execute at @e[tag=grindersummon,type=minecraft:item_frame] run summon armor_stand ~ ~-0.85 ~ {NoGravity:1b,Marker:1b,Invisible:1b,PersistenceRequired:1b,Tags:["grinder"],DisabledSlots:4144959,ArmorItems:[{},{},{},{id:"minecraft:stone",Count:1b}]}
execute at @e[tag=grindersummon,type=minecraft:item_frame] run setblock ~ ~1 ~ minecraft:dropper[facing=up]{CustomName:"{\"text\":\"Grinder\",\"color\":\"dark_gray\",\"bold\":\"true\"}"} replace
execute at @e[tag=grindersummon,type=minecraft:item_frame] run playsound minecraft:block.grindstone.use master @a ~ ~ ~ 1 0 1
execute as @e[tag=grindersummon,type=minecraft:item_frame] run kill @e[tag=grindersummon,type=minecraft:item_frame]
#Effects
execute as @e[tag=grinder,type=minecraft:armor_stand] at @s run tp @e[tag=grinder,type=minecraft:armor_stand] ~ ~ ~ ~5 ~
